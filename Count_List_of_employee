// program to read salary of 10 employees and store them in an array and count the number of employee whose salary between 10000 to 20000
#include <stdio.h>
int main()
{
    int salary[10];
    int count = 0;
    for (int i = 0; i < 10; i++)
    {
        printf("Enter the salary of employee %d: ", i + 1);
        scanf("%d", &salary[i]);
        if (salary[i] >= 10000 && salary[i] <= 20000)
        {
            count++;
        }
    }
    printf("The number of employee whose salary between 10000 to 20000 is %d\n", count);
    return 0;
}
